# ========================================
# AI Music Pro - Production Environment
# ========================================
# Copy this file to .env and fill in your values
# ========================================

# ----------------------------------------
# Suno API Configuration
# ----------------------------------------
SUNO_API_URL=https://open.suno.cn
SUNO_ACCESS_KEY=your_suno_access_key_here

# ----------------------------------------
# GLM API Configuration (External LLM)
# ----------------------------------------
GLM_API_URL=https://open.bigmodel.cn/api/paas/v4
GLM_AUTHORIZATION=your_glm_authorization_token
GLM_MODEL=glm-5
GLM_MODEL_FAST=glm-4-flash

# ----------------------------------------
# JoyBuilder API Configuration (JD Internal)
# ----------------------------------------
JOYBUILDER_API_URL=https://api.joybuilder.jd.com/v1
JOYBUILDER_API_KEY=your_joybuilder_api_key
JOYBUILDER_MODEL_LYRICS=deepseek-v3.2
JOYBUILDER_MODEL_STYLE=qwen3-8b
JOYBUILDER_MODEL_CREATIVE=deepseek-r1-0528
JOYBUILDER_MODEL_FAST=joyai-flash

# ----------------------------------------
# Network Environment
# internal: Use JoyBuilder (JD network)
# external: Use GLM (public network)
# ----------------------------------------
NETWORK_ENV=external

# ----------------------------------------
# Database Configuration
# ----------------------------------------
PG_USER=postgres
PG_PASSWORD=your_secure_password
PG_DATABASE=music_ai
PG_PORT=5432

# ----------------------------------------
# Redis Configuration
# ----------------------------------------
REDIS_PORT=6379

# ----------------------------------------
# JWT Configuration
# ----------------------------------------
JWT_SECRET=your_very_secure_jwt_secret_key_here

# ----------------------------------------
# Logging
# ----------------------------------------
LOG_LEVEL=info

# ----------------------------------------
# Ports (optional, defaults shown)
# ----------------------------------------
FRONTEND_PORT=80
BACKEND_PORT=3000
